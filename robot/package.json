{
  "name": "tinkoff-robot-server",
  "version": "1.0.0",
  "description": "Tinkoff Contest",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./src/index.ts",
  "scripts": {
    "dev": "nodemon -e ts,js ./src/index.ts",
    "build": "tsc",
    "start": "node ./build/index.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "eslint . --fix --ext .js,.ts",
    "prettier": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "proto": "grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=esModuleInterop=true --ts_proto_out=./src/tinkoff/contracts --ts_proto_opt=outputServices=generic-definitions,useExactTypes=false -I ./protos/ ./protos/*.proto"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "grpc-tools": "^1.11.2",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-proto": "^1.112.1",
    "typescript": "^4.6.4"
  }
}
